{
  "manifest_version": 3,
  "name": "Card Detail Extractor",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://mytest.com/nfi/CardBudgetDraft*"],
      "js": ["contentScript.js"],
      "run_at": "document_end"
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "images/icon48.png",
    }
  }
}
